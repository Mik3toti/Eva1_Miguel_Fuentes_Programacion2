<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/Total"
        android:layout_width="93dp"
        android:layout_height="19dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:text="0"
        android:enabled="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <TextView
        android:id="@+id/NombreComida1"
        android:layout_width="109dp"
        android:layout_height="24dp"
        android:layout_marginTop="60dp"
        android:text="Pastel de choclo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="203dp"
        android:layout_height="251dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/restaurant"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/PastelLogo"
        android:layout_width="102dp"
        android:layout_height="177dp"
        app:layout_constraintBottom_toTopOf="@+id/CazuelaLogo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976"
        app:srcCompat="@drawable/pastel_de_choclo" />

    <ImageView
        android:id="@+id/CazuelaLogo"
        android:layout_width="107dp"
        android:layout_height="119dp"
        android:layout_marginBottom="144dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/cazuela" />

    <TextView
        android:id="@+id/NombreComida2"
        android:layout_width="65dp"
        android:layout_height="21dp"
        android:layout_marginTop="156dp"
        android:text="Cazuela"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/NombreComida1" />

    <EditText
        android:id="@+id/NumeroComida1"
        android:layout_width="62dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="numberSigned"
        android:shadowColor="#120000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/NumeroComida2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.085"
        app:layout_constraintStart_toEndOf="@+id/Cantidad1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.743" />

    <TextView
        android:id="@+id/Cantidad1"
        android:layout_width="47dp"
        android:layout_height="27dp"
        android:layout_marginTop="16dp"
        android:text="Cant."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.414"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/NombreComida1" />

    <TextView
        android:id="@+id/Cantidad2"
        android:layout_width="50dp"
        android:layout_height="24dp"
        android:layout_marginTop="8dp"
        android:text="Cant."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/NombreComida2" />

    <EditText
        android:id="@+id/NumeroComida2"
        android:layout_width="62dp"
        android:layout_height="41dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="184dp"
        android:ems="10"
        android:inputType="numberSigned"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/Cantidad2" />

    <TextView
        android:id="@+id/TotalComida2"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginBottom="148dp"
        android:ems="10"
        android:text="0"
        android:enabled="false"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.603"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/TotalComida1"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginBottom="292dp"
        android:ems="10"
        android:text="0"

        android:shadowColor="#120000"
        android:enabled="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.603"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="608dp"
        android:text="Comida"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.41"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Propina"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.415"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.921" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/total"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.409"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.567" />

    <TextView
        android:id="@+id/ComidaTotal"
        android:layout_width="90dp"
        android:layout_height="22dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:text="0"

        android:enabled="false"
        app:layout_constraintBottom_toTopOf="@+id/Propina"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/Propina"
        android:layout_width="94dp"
        android:layout_height="21dp"
        android:ems="10"
        android:text="0"
        android:enabled="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.361"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.921" />

    <Switch
        android:id="@+id/switchPropina"
        android:layout_width="69dp"
        android:layout_height="21dp"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="56dp"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>